(function ($, Drupal, window, document, undefined) {

  // -------------------------
  // jQuery Document ready.
  // -------------------------
  $(document).ready(function() {
    // @TODO Move in the appropriate behavior.

    // Close custom overlays.
    $('[data-action=closeOverlay]').once().click(function(e) {
      e.preventDefault();
      $('body').removeClass('no-scroll');
      $('.overlay').fadeOut();
    });

    $('.owl-process').once('jquery-once-owl').owlCarousel({
      loop: false,
      margin: 0,
      nav: true,
      autoHeight: true,
      responsive: {
        0: {
          items: 1
        },
        600: {
          items: 1
        },
        800: {
          items: 2
        },
        1000: {
          items: 3
        },
        1400: {
          items: 4
        }
      }
    });

    $('.owl-gallery').once('jquery-once-owl').owlCarousel({
      loop: false,
      margin: 0,
      nav: true,
      responsive: {
        0: {
          items: 1
        }
      }
    });

    // -------------------------
    // Manuals carousel.
    // -------------------------
    if ($('.owl-manuals').once('jquery-once-owl').size()) {
      $('.owl-manuals').owlCarousel({
        loop: false,
        margin: 0,
        nav: true,
        dots: true,
        responsive:{
          0: {
            items:1
          },
          640: {
            items:2
          },
          900: {
            items:3
          },
          1281: {
            items:4
          }
        }
      });
    }

  });
  // DOCUMENT READY END


  // ------------------------------------
  // FOUNDATION REVEAL SPECIFIC BEHAVIOUR
  // ------------------------------------
  Drupal.behaviors.zurbReveal = {
    attach: function (context, settings) {
      /**
       * Open status messages in a reveal overlay.
       */
      if ($('#status-messages-in-reveal').size()) {
        $('#status-messages-in-reveal').foundation('open');
      }
    }
  };

  // ---------------------
  // GLOBAL BEHAVIOURS
  // ---------------------
  Drupal.behaviors.caleffiGlobals = {
    attach: function (context, settings) {

      /* ---------------------------------------------------------------- */
      /* HELPER FUNCTIONS                                                 */
      /* ---------------------------------------------------------------- */

      /**
       * Remove the link from a linked button.
       *
       * @param jQueryObject $button
       *   the link to disable
       */
      function unsetHref($button) {
        var href = $button.attr('href')
        $button.removeAttr('href').attr('data-href', href);
      }

      /**
       * Passing an url to this function it will remove the scheme.
       */
      function unSchemeUrl(url) {
        var re = /^http:/;
        return url.replace(re, '');
      }

      /* ---------------------------------------------------------------- */

      // Caching.
      var $window = $(window),
          $body = $('body'),
          $header = $('.site-header', $body),
          $section = $('body > SECTION'),
          $main = $('main', $body),
          $footer = $('.site-footer', $body);

      // Header menu.
      var $menuCorporate = $(".corporate", $header),
          menuCorporateH = $menuCorporate.outerHeight(),
          $menuMain = $('.main-menu-wr', $header).find('.site-main-menu'),
          menuMainH = $('.main-menu-wr', $header).outerHeight();

      // Resizing variables.
      var window_width = $window.width(),
          window_height = $window.height();

      // Video player with image placeholder.
      $('[data-action="playVideo"]').click(function () {

        var $videoWrapper = $(this).next('.video-wrapper');
        var $iframe = $videoWrapper.find('iframe');
        $iframe.attr('src', $iframe.data('src')).attr('width', '100%').attr('height', '340');
        $(this).fadeOut();
        $videoWrapper.removeClass('hidden');
      });
      $('[data-action="playVideoPage"]').click( function(){
        var iframe = $(this).parent('.video').find('iframe');
        iframe[0].src += '?autoplay=1';
        $(this).hide();
        $(this).parent('.video').find('picture').hide();
        iframe.removeClass('hidden');
      });

      // ------------------------------
      // Header main and mobile menu interaction.
      // ------------------------------
      $header.once( function() {

        // Open main menu.
        var $main_menu_expandable_elements_trigger = $menuMain.find('.expanded > a');
        var $mobile_menu_expandable_elements_trigger = $('.mobile-menu', $body).find('.site-main-menu').find('.expanded > a');
        var $main_menu_expandable_elements_trigger = $.merge($main_menu_expandable_elements_trigger, $mobile_menu_expandable_elements_trigger);
        var $main_menu_submenus = $main_menu_expandable_elements_trigger.next('.menu');

        $main_menu_expandable_elements_trigger.click(function(e){
          e.preventDefault();
          var $this = $(this);
          if ($this.next(".menu").is(":visible")){
            $this.next(".menu").fadeOut();
            $this.removeClass("is-active");
          }
          else {
            $main_menu_expandable_elements_trigger.filter('.is-active').removeClass('is-active');
            $main_menu_submenus.fadeOut();
            $main_menu_submenus.filter('.is-active').removeClass('is-active');
            $this.next(".menu").fadeIn();
            $this.addClass('is-active');
          }
        });

        // Open the search bar in the main menu.
        $('[data-action="openSearch"]').click(function(e) {
          e.preventDefault();
          var target = '#' + $(this).data('target');
          $(this).toggleClass('is-open');
          $(target).toggleClass('is-open');
          $('[data-action="openMobileSearch"]').toggleClass('is-open');
        });

        $('[data-action="openMobileSearch"]').click(function(e) {
          e.preventDefault();
          var target = '#' + $(this).data('target');
          $(this).toggleClass('is-open');
          $(target).toggleClass('is-open');
          $('[data-action="openSearch"]').toggleClass('is-open');
        });

        // Show social.
        $('[data-action="showSocial"]').click(function(e) {
          e.preventDefault();
          var $this = $(this);
          if($('.social-content').hasClass('hidden')) {
            $this.addClass('is-active');
            $('.social-content').fadeIn(function() {
              $(this).removeClass('hidden');
            });
          }
          else {
            $this.removeClass('is-active');
            $('.social-content').fadeOut(function(){
              $(this).addClass('hidden');
            });
          }
        });
        // Close social clicking on X.
        $('[data-action="closeSocial"]').click(function(e){
          e.preventDefault();
          $('.social-content').fadeOut(function(){
            $(this).addClass('hidden');
          });
        });

        // Mobile menu socials interaction.
        $('.site-header_social-mobile_toggler').click(function(e) {
          var $toggler = $(this),
              $links_container = $toggler.parents('.site-header_social-mobile').find('.site-header_social-mobile_content');
          if ($links_container.hasClass('hidden')) {
            $toggler.addClass('is-active');
            $links_container.fadeIn(function() {
              $(this).removeClass('hidden');
            });
          }
          else {
            $toggler.removeClass('is-active');
            $links_container.fadeOut(function(){
              $(this).addClass('hidden');
            });
          }
        })

      }); // HEADER CODE END.

      // Prevent users from clicking the last breadcrumb item (that is the current page).
      $('.breadcrumb', $body).once().find('.last:not(.first)').find('a').on('click ', function() {
        return false;
      });

      // Set events on window scroll.
      $window.scroll(function() {
        // Header menu.
        var scrollTop = $window.scrollTop();
        if (scrollTop >= menuCorporateH) {
          $header.addClass('is-fix');
        }
        else {
          $header.removeClass('is-fix');
        }
        if ($('.page-menu').length > 0) {
          positionBreadcrumbsCatalog();
        }
      });

      // ------------------------------
      // HOMEPAGE
      // ------------------------------
      if ($('.node-homepage-full', $main).once().size()) {

        // Add a fade in up effect to the content of the first slide
        // of the main carousel.
        $('.home-carousel', $main).find('li').eq(0).find('.description').viewportChecker({
          classToAdd: 'visible animated fadeInUp',
          classToRemove: 'opacity',
          offset: -100
        });
        // Add a class to the elements when they are visibles.
        $('.block-post').viewportChecker({
          classToAdd: 'visible animated fadeIn',
          classToRemove: 'opacity',
          offset: -100
        });
        $('.visual-post').viewportChecker({
          classToAdd: 'visible animated bounceInRight',
          classToRemove: 'opacity',
          offset: 300
        });

        // Main carousel.
        if ($('.home-carousel', $main).size()) {
          var custom_navigation = '<div class="custom-navigation">\n\
  <a href="#" class="flex-prev"><span class="icon-arrow-left2"></span></a>\n\
  <a href="#" class="flex-next"><span class="icon-arrow-right2"></span></a>\n\
  </div>';
          $('.home-carousel', $body).append(custom_navigation).find('.flexslider')
            .flexslider({
              animation: 'slide',
              customDirectionNav: $('.custom-navigation a')
            });
        }

        // News carousel.
        if ($('.owl-news', $main).size()) {
          $('.owl-news').owlCarousel({
            loop: false,
            margin: 0,
            nav: true,
            responsive: {
              0: {
                items: 1
              },
              480: {
                items: 2
              },
              768: {
                items: 3
              },
              1024: {
                items: 4,
                nav: false
              }
            }
          });
        }

        $('[data-action=playVideo]').click( function(){
          var videoWrapper = $(this).next('.video-wrapper');
          var iframe = videoWrapper.find('iframe');
          iframe[0].src += '?autoplay=1';
          $(this).fadeOut();
          videoWrapper.removeClass('hidden');
        });

      } // Homepage END.

      // ------------------------------
      // SERIE
      // ------------------------------
      if ($body.hasClass('node-type-serie')) {

        // Panel switch.
        if ($('[data-action="switchContent"]').size()) {
          $('[data-action="switchContent"]').once().click(function (e) {
            e.preventDefault();
            var target = $(this).attr('href');
            $('.switch-container').hide(0);
            $('[data-action="switchContent"]').removeClass('is-active');
            $(this).addClass('is-active');
            $(target).show(0, function() {
              reinitFoundationEqualizer();
            });
          });
