  // ------------------------
  // CATALOGUE AND AREA MENU
  // ------------------------
  Drupal.behaviors.caleffiCatalogue = {
    attach: function (context, settings) {

      // Variables caching.
      var $window = $(window),
          $area_menu = $('.area-menu'),
          $catalogue = $('#caleffi-product-catalogue'),
          $catalogue_sections = $catalogue.find('.product-catalogue_section'),
          $active_catalogue_section = $('#caleffi-product-catalogue').find('.active').parents('.product-catalogue_section');

      // ----------------------------
      // AREA MENU INTERACTION.
      // ----------------------------

      // Helper function, return FALSE if we are in catalogue page
      // in which the area menu should work as normal links.
      function isCatalogueLandind() {
        if ($('#caleffi-product-catalogue').find('[data-area]').size()) {
          return true;
        }
        else {
          return false;
        }
      }

      if ($area_menu.once().size()) {

        $area_menu.find('a').click(function(e) {
          // If we are in an internal section, the click will load the
          // catalogue landing, so we can return the default link behavior.
          if (!isCatalogueLandind()) {
            return true;
          }
          // Otherwise the area menu will filters the catalogue sections.
          e.preventDefault();
          var $this = $(this);

          // Recupero l'hash
          var url = $this.attr('href');
          var hash = url.substring(url.indexOf('#') + 1);
          // Recupero il testo per aggiungero al title della pagina
          var text = $this.text();

          if ($this.hasClass('is-active')) {
            // Se clicco su di una voce già selezionata, disattivo tutte le selezioni
            $this.removeClass('is-active');
            $catalogue_sections.removeClass('is-disabled');
            // Aggiorno l'hash dell'url della pagina
            if (history.pushState) {
              history.pushState(null, null, '#');
            }
            else {
              location.hash = '';
            }
            // window.location.href = window.location.origin + window.location.pathname;
            // Aggiorno il titolo della pagina
            $('.page-title').find('span').remove();
          }
          else {
            // Ho cliccato su di una voce non selezionata
            // Rimuovo voci di menu eventualmente attive
            $area_menu.find('.is-active').removeClass('is-active');
            // Attivo la voce di menu cliccata
            $this.addClass('is-active');
            var area = $this.data('area');

            $catalogue_sections.removeClass('is-disabled');
            // Controllo che vi sia almeno una voce da mantenere attiva
            // prima di nasconderle, onde evitare di presentare un catalogo
            // vuoto nel caso si siano verificati problemi con il data-area
            if ($catalogue_sections.filter('[data-area="' + area + '"]').size()) {
              $catalogue_sections.not('[data-area="' + area + '"]').addClass('is-disabled');
            }
            // Aggiorno l'hash dell'url della pagina
            window.location.hash = hash;
            // Aggiorno il titolo della pagina
            $('.page-title').find('span').remove().end().append('<span> - ' + text + '</span>');
          }
          return false;
        });

        // Se arrivo con un URL con già selezionato un hash, attivo l'area
        // Il ciclo su tutti gli elementi del menu non è ottimale, ma
        // trattandosi di sole 5 voci il costo è del tutto trascurabile.
        if (window.location.hash !== '') {
          $area_menu.find('a').each(function() {
            var url = $(this).attr('href');
            var hash = url.substring(url.indexOf('#') + 1);
            if (window.location.hash === '#' + hash) {
              $(this).trigger('click');
            }
          });
        }

        if (typeof(Drupal.settings.Caleffi.area_name) !== 'undefined') {
          var current_area_name_array = Drupal.settings.Caleffi.area_name;
          for (var i = 0; i < current_area_name_array.length; i++) {
            $area_menu.find('a')
              .filter('[data-area="' + current_area_name_array[i] + '"]')
              .addClass('is-active');
          };
        };

      }; // END AREA MENU SPECIFIC CODE


      // ----------------------------
      // PRODUCTS CATALOG
      // ----------------------------

      // JS interaction logic is needed only on interal catalogue pages,
      // not on catalogue landing or sublandings (i.e. SC landing).
      if ($('#caleffi-product-catalogue').size() && $active_catalogue_section.once().size()) {

        /**
         * Helper function that split an unordered list in two separated list.
         */
        function splitUnorderedListInTwo($list_to_split) {
          var $list_items = $list_to_split.find('> li'),
              list_items_count = $list_items.size();
          // If we have less than 6 elements, we retain the original one column
          // layout.
          if (list_items_count <= 6) {
            return;
          }
          var $second_column = $('<ul></ul>');
          // Get the same classes of the original list.
          $second_column.attr('class', $list_to_split.attr('class'));
          $list_items.each(function() {
            var $this = $(this);
            if (($this.index() + 1) <= Math.round(list_items_count / 2)) {
              return;
            }
            $this.detach().appendTo($second_column);
          });
          $second_column.appendTo($list_to_split.parent());
        }

        // OPEN CLOSE THE SERIE OF PRODUCTS
        // The default behavior is to allow click on the single section tile;
        // this will reload the page with the section content loaded.
        // If the section is active, we must disable the click on the section
        // tile and expand / collapse the section content.
        // When opening a page with an active section, the section must be
        // automatically expanded.
        var $active_catalogue_section_link = $active_catalogue_section.find('.active'),
            $section_families_wrapper = $active_catalogue_section_link.next('.product-catalogue_section-families'),
            section_families_wrapper_margin = 16; // 16px is the margin bottom added to expanded element.

        // Get the families list to split in two.
        var $section_families_list = $section_families_wrapper.find('.product-catalogue_section-families_menu');
        splitUnorderedListInTwo($section_families_list);

        // We must check heights after the splitting of families in two columns.
        var section_families_wrapper_height = $section_families_wrapper.outerHeight(),
            section_tile_height = $active_catalogue_section_link.outerHeight();

        // OPEN THE PRODUCTS SECTION.
        // Manage the click event on the active element.
        $active_catalogue_section_link.click(function(e) {
          e.preventDefault();

          // Set manually the width on the element to prevent strange behavior
          // during animation on mobile and iOS devices.
          $section_families_wrapper.css('width', $('#boxes-box-static_area_menu').width());

          if ($active_catalogue_section.hasClass('is-open')) {
            $active_catalogue_section.removeClass('is-open');
            $active_catalogue_section.velocity({
              'height' : section_tile_height
            }, {
              'duration' : 200
            });
            $section_families_wrapper.velocity('slideUp', { duration : 200 });
          }
          else {
            $active_catalogue_section.addClass('is-open');
            $active_catalogue_section.velocity({
              'height' : section_tile_height + section_families_wrapper_height + section_families_wrapper_margin
            }, {
              'duration' : 400
            });
            $section_families_wrapper.velocity('slideDown', { duration : 400 });
          }
        });

        // OPEN THE PRODUCTS FAMILY.
        // In a section page we can also expand the single family by clicking
        // on the family name.
        $active_catalogue_section.find('h4').parent().click(function(e) {
          e.preventDefault();
          $(this).parent().toggleClass('is-family-open');
          section_families_wrapper_height = $(this).parents('.product-catalogue_section-families').outerHeight();
          $active_catalogue_section.velocity({
            'height' : section_tile_height + section_families_wrapper_height + section_families_wrapper_margin
          }, {
            'duration' : 200
          });
          return false;
        })

        // AUTOSCROLL ON SECTION PAGE LOAD.
        // Scroll to the section and trigger the click on the first page load.
        $.scrollTo($active_catalogue_section_link, {
          axis: 'y',
          duration: 800,
          onAfter: function() {
            $active_catalogue_section_link.trigger('click');
          }
        });

        // ---------------------------
        // ON RESIZE EVENTS
        // ---------------------------
        function onResize() {
          // Aggiorno le variabili di dimensione
          // window_width = $window.width();
          // window_height = $window.height();
          $section_families_wrapper.css('width', $('#boxes-box-static_area_menu').width());

          // We must check heights after the splitting of families in two columns.
          section_families_wrapper_height = $section_families_wrapper.outerHeight(),
          section_tile_height = $active_catalogue_section_link.outerHeight();
          $active_catalogue_section.velocity({
            'height' : section_tile_height + section_families_wrapper_height
          }, {
            'duration' : 400
          });
        }

        // Bind the onResize function to the resize event.
        $window.on('resize', $.debounce(200, onResize));
        // Execute the onResize function even on device rotation.
        window.onorientationchange = onResize;

      } // End of if

    }}; // END OF caleffiCatalogue BEHAVIOR.


